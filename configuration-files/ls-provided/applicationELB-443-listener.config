# This file creates a Listener on 443 and adds the certificate via an ARN on an Application Load Balancer
option_settings:
  aws:elbv2:listener:443:
    DefaultProcess: https
    ListenerEnabled: 'true'
    Protocol: HTTPS
    SSLCertificateArns: arn:aws:acm:us-west-2:000000000000:certificate/00000000-0000-0000-0000-000000000000
  aws:elasticbeanstalk:environment:process:https:
    Port: '443'
    Protocol: HTTPS